<mvc:View
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:form="sap.ui.layout.form"
    xmlns:smartForm="sap.ui.comp.smartform"
    xmlns:smartField="sap.ui.comp.smartfield"
    controllerName="ui5.ogarpt.controller.formSections.SectionC"
>
    <smartForm:SmartForm
        id="_IDGenSmartForm2"
        validationMode="Async"
        editable="true"
    >
        <smartForm:Group id="_IDGenGroup4">
            <smartForm:GroupElement id="_IDGenGroupElement9">
                <smartField:SmartField
                    id="_IDGenSmartField8"
                    value="{Price}"
                />
            </smartForm:GroupElement>
            <smartForm:GroupElement id="_IDGenGroupElement10">
                <smartField:SmartField
                    id="_IDGenSmartField9"
                    value="{CurrencyCode}"
                />
            </smartForm:GroupElement>
        </smartForm:Group>
    </smartForm:SmartForm>
    
    <Table
        id="EditableTable"
        growing="true"
        growingThreshold="10"
        paste="onPaste"
    >
        <headerToolbar>
            <OverflowToolbar id="otbSubheader">
                <Title
                    id="_IDGenTitle1"
                    text="Products Items"
                    level="H2"
                />
                <ToolbarSpacer id="_IDGenToolbarSpacer" />
                <Button
                    id="addButton"
                    text="Add"
                    type="Transparent"
                    press="handleAdd"
                />
                <Button
                    id="editButton"
                    text="Edit"
                    type="Transparent"
                    press="handleEdit"
                />
                <!-- <Button
                    id="saveButton"
                    text="Save"
                    type="Transparent"
                    press="onSave"
                    visible="false"
                />
                <Button
                    id="cancelButton"
                    text="Cancel"
                    type="Transparent"
                    press="onCancel"
                    visible="false"
                /> -->
            </OverflowToolbar>
        </headerToolbar>
        <columns>
            <Column id="_IDGenColumn">
                <Text
                    id="_IDGenText"
                    text="Product ID"
                />
            </Column>
            <Column
                id="_IDGenColumn1"
                minScreenWidth="Tablet"
                demandPopin="true"
                hAlign="End"
            >
                <Text
                    id="_IDGenText1"
                    text="Product Name"
                />
            </Column>
            <Column
                id="_IDGenColumn2"
                minScreenWidth="Tablet"
                demandPopin="true"
                hAlign="Center"
            >
                <Text
                    id="_IDGenText2"
                    text="Price"
                />
            </Column>
            <Column
                id="_IDGenColumn4"
                minScreenWidth="Tablet"
                demandPopin="true"
                hAlign="Center"
            >
                <Text
                    id="_IDGenText3"
                    text="Currency"
                />
            </Column>
        </columns>
        <items>
            <ColumnListItem
                id="_IDGenColumnListItem"
                vAlign="Middle"
            >
                <!-- <Input id="_IDGenInput1" value="{ProductId}" editable="false" />
                <Input id="_IDGenInput2" value="{ProductName}" />
                <Input id="_IDGenInput3" value="{Price}" type="Number" />
                <Input id="_IDGenInput4" value="{CurrencyCode}" type="Number" /> -->
            </ColumnListItem>
        </items>
    </Table>
</mvc:View>
